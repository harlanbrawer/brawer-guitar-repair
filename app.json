{
  "expo": {
    "name": "brawer-guitar-repair",
    "slug": "brawer-guitar-repair",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/guitar_favicon.png",
      "output": "static",
      "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "baseUrl": "/brawer-guitar-repair"
    }
  }
}
